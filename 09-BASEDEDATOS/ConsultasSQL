--EJEMPLO DE SUBCONSULTA
SELECT * FROM USUARIOS WHERE ID=(
SELECT MAX(USUARIO_ID) FROM ENTRADAS);
--SACAR TODAS LAS ENTRADAS DE ACCION UTILIZANDO EL NOMBRE
SELECT * FROM ENTRADAS WHERE CATEGORIA_ID=(
SELECT ID FROM CATEGORIAS 
WHERE NOMBRE = 'Accion');

--MOSTRAR LAS CATEGORIAS CON MAS DE 3 ENTRADAS
SELECT * FROM CATEGORIAS 
WHERE ID IN (
SELECT CATEGORIA_ID FROM ENTRADAS
GROUP BY CATEGORIA_ID
HAVING COUNT(*) IN 
(SELECT COUNT(*) FROM ENTRADAS 
GROUP BY CATEGORIA_ID 
HAVING COUNT(*)>=3
ORDER BY 1 DESC 
))